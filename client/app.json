{
  "expo": {
    "runtimeVersion": "1.0.1",
    "name": "SpotSportv1",
    "slug": "spotsport",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/spotsport.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/spotsport.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mauriidg.AythenSport",
      "versionCode": 1,
      "config": {
        "googleMaps": {
          "apiKey": "@env.GOOGLE_MAPS_API_KEY"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "",
          "enableGooglePay": true
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "d88586a8-1975-4f6f-8faf-c1868ce59d87"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/ac124ba8-62c5-44ac-ab0e-8da7a8b8e646"
    },
    "owner": "mauriidg"
  }
}
